@{
    ViewBag.Title = "Index";
}

<h2>Login page</h2>

<section id="loginWrapper">
    <p><label>User Name:</label> <input type="text" data-bind="value:UserName" /></p>
    <p><label>Password:</label> <input type="text" data-bind="value:Password" /></p>

   <p><input type="button" value="Login" data-bind="click:LogIn" /></p>

    <div data-bind="visible:LoginSuccess">
        you have successfully login
    </div>
</section>
